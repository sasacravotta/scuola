9/Dato il numeratore e il denominatore di una frazione, stabilire se tale frazione è propria, impropria o apparente. 

public class Main
{
	public static void main(String[] args) {
	    int numeratore=7;
	    int denominatore=5;
	    
	    if (numeratore<denominatore)
	    {
	        System.out.println("La frazione "+numeratore+"/"+denominatore+" è propria.");
	    }
	    
	    else if (numeratore>denominatore)
	    {
	       System.out.println("La frazione "+numeratore+"/"+denominatore+" è impropria.");
	    }
	    else 
	    {
	        System.out.println("La frazione "+numeratore+"/"+denominatore+" è apparente.");
	    }
	}
}





10//Dato due numeri interi, stabilire, senza effettuare calcolo, se la loro somma è pari o dispari.
public class Main
{
	public static void main(String[] args) {
	    int b=7;
	    int a=3;
	    
	    if ((a%2==0 && b%2==0) || (a%2!=0 && b%2!=0))
	    {
	        System.out.println("La somma di "+a+" e "+b+" è pari."); 
	    }
	    
	    else 
	    {
	     System.out.println("La somma di "+a+" e "+b+" non è pari.");   
	    }
	    
	}
}

11//Data la base e l'esponente intero di una potenza, stabilire senza effettuare calcolo, il segno di tale potenza.

public class Main
{
	public static void main(String[] args) {
	    int a=-3;
	    int n=3;
	    
	    if (a>0 || n%2==0)
	    {
	        System.out.println("Il risultato della potenza "+a+"^"+n+" sarà di segno positivo"); 
	    }
	    else 
	    {
	        System.out.println("Il risultato della potenza "+a+"^"+n+" sarà di segno negativo");   
	    }
	    
	}
}

12 //Determinare se la potenza a^n è un numero pari (Senza effettuare l'operazione)

//Data la base intera e l'esponente intero di una potenza, stabilire senza effettuare calcolo, il segno della potenza.

public class Main
{
	public static void main(String[] args) {
	    int a=5;
	    int n=6;
	    
	    if (a%2==0 || n%2==0)
	    {
	        System.out.println("L'espressione "+a+" elevato alla "+n+"^ darà un risultato pari");
	    }
	    
	    else 
	    {
	      System.out.println("L'espressione "+a+" elevato alla "+n+"^ darà un risultato dispari");
	    }
	    
	}
}



